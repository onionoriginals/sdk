# Turnkey Configuration
# Get these from https://app.turnkey.com
TURNKEY_ORGANIZATION_ID=your_turnkey_organization_id
TURNKEY_API_PUBLIC_KEY=your_turnkey_api_public_key
TURNKEY_API_PRIVATE_KEY=your_turnkey_api_private_key

# JWT Authentication
# Generate a secure random string (32+ characters)
# Example: openssl rand -base64 32
JWT_SECRET=your_jwt_secret_here_at_least_32_chars
JWT_EXPIRES_IN=7d

# DID Configuration
# Domain used in did:webvh identifiers
# For development: localhost:5000
# For production: your-domain.com
DID_DOMAIN=localhost:5001
VITE_APP_DOMAIN=localhost:5001

# Google OAuth (optional)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI=http://localhost:5000/api/auth/google/callback

# Database Configuration (if using PostgreSQL)
# DATABASE_URL=postgresql://user:password@localhost:5432/originals_explorer

# Server Configuration
PORT=5000
NODE_ENV=development
